<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fykj.scaffold.security.business.mapper.ApiManageMapper">

    <update id = "updateStatusById" >
        update api_manage
        set api_manage.status = #{status}
        where api_manage.id = #{id}
    </update>
</mapper>
