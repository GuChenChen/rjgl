<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fykj.scaffold.cms.mapper.CmsContentMapper">

    <update id="updateActualReadingById">
        update cms_content
        set actual_reading = actual_reading+1
        where id=#{id}
    </update>
</mapper>
